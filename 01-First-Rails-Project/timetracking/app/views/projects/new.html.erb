<h1>New Project:</h1>

<% if @project.errors.messages.present? %>
  <%= @project.errors.messages %>
<% end %>

<%= form_for @project do |f| %>
  <div>
  <%= f.label :title %>
  <%= f.text_field :title %>  
  </div>
  <div>
  <%= f.label :description %>
  <%= f.text_area :description %>
  </div>
  <div>
  <%= f.submit "Create" %>  
  </div>
<% end %>