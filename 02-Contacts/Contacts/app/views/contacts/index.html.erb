<h1>Contact list</h1>

<table style="border:1px solid black;">
  <tr>
    <td>Name</td>
    <td>Address</td>
    <td>Phone Number</td>
    <td>Email Address</td>
  </tr>

  <% @contacts.each do |person| %>
    <tr>
      <td><%= person.name %></td>
      <td><%= person.address %></td>
      <td><%= person.phone %></td>
      <td><%= person.email %></td>
    </tr>
  <% end %>
</table>