<div class="jumbotron">
  <h1><%= @heading %></h1>
  <!-- <p>...</p> -->
  <!-- <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a></p> -->
</div>

<table>
  <tr>
    <td>Band</td>
    <td>Venue</td>
    <td>City</td>
    <td>Date</td>
    <td>Price</td>
    <td>Description</td>
  </tr>
  <% @concerts.each do |concert| %>
  <tr>
    <td><%= link_to concert.band, concert_path(concert) %></td>
    <td><%= concert.venue %></td>
    <td><%= concert.city %></td>
    <td><%= concert.date %></td>
    <td><%= concert.price %></td>
    <td><%= concert.description %></td>
  </tr>
  <% end %>
</table>